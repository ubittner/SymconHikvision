  "actions": [
    {
      "type": "Label",
      "caption": "(0) Instance information"
    },
    {
      "type": "Button",
      "caption": "Manual",
      "onClick": "echo 'https://github.com/ubittner/SymconHikvision';"
    },
    {
      "type": "Label",
      "caption": "(2) Camera"
    },
    {
      "type": "Button",
      "caption": "Display camera information",
      "onClick": "$info = HV_GetCameraInformation($id); print_r($info);"
    },
    {
      "type": "Button",
      "caption": "Reset people counter",
      "onClick": "HV_ResetPeopleCounter($id);"
    },
    {
      "type": "Button",
      "caption": "Display message list",
      "onClick": "HV_DisplayMessageList($id);"
    },
    {
      "type": "RowLayout",
      "items": [
        {
          "type": "Button",
          "caption": "Alarm output 1 off",
          "onClick": "HV_TriggerCameraAlarmOutput($id, 1, false);"
        },
        {
          "type": "Button",
          "caption": "Alarm output 1 on",
          "onClick": "HV_TriggerCameraAlarmOutput($id, 1, true);"
        }
      ]
    }
  ],
